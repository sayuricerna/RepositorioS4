                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>    SELECT 
        L.Titulo AS Libro,
        L.Autor AS Autor,
        SUM(V.Cantidad) AS Total_Vendidos,
        SUM(V.Total) AS Total_Ingresos
    FROM Ventas V
