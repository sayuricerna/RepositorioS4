                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>    SELECT 
        L.Titulo AS Libro,
        L.Autor AS Autor,
        SUM(V.Cantidad) AS Total_Vendidos,
        SUM(V.Total) AS Total_Ingresos
    FROM Ventas V
    INNER JOIN Libros L ON V.IdLibro = L.IdLibro
    WHERE YEAR(V.FechaVenta) = 2025 AND MONTH(V.FechaVenta) = @Mes
    GROUP BY L.IdLibro, L.Titulo, L.Autor
    ORDER BY Total_Vendidos DESC;</CommandText>
